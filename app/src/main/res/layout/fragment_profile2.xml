<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchFragment">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/ic_profile5"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_margin="4dp"
        android:src="@drawable/img_sample2"
        app:layout_constraintStart_toStartOf="@id/ic_profile_margin5"
        app:layout_constraintTop_toTopOf="@id/ic_profile_margin5"
        app:shapeAppearance="@style/ShapeAppearance.Material3.Corner.Full">

    </com.google.android.material.imageview.ShapeableImageView>

    <ImageView
        android:id="@+id/ic_profile_margin5"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:src="@drawable/ic_coloredcircle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </ImageView>
</androidx.constraintlayout.widget.ConstraintLayout>