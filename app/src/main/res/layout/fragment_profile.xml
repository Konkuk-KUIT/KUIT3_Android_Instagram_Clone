<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment">

    <TextView
        android:id="@+id/tv_profile_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="id 바뀌는 곳"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="25dp"
        android:textColor="@color/black"
        android:layout_marginStart="20dp"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_threads"
        app:layout_constraintStart_toEndOf="@id/tv_profile_id"
        android:layout_marginStart="101dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="25dp"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_create"
        app:layout_constraintStart_toEndOf="@id/tv_profile_id"
        android:layout_marginStart="145dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="25dp"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_menu"
        app:layout_constraintStart_toEndOf="@id/tv_profile_id"
        android:layout_marginStart="189dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="25dp"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv_profile_img"
        android:layout_width="79dp"
        android:layout_height="79dp"
        android:src="@drawable/img_sample"
        app:layout_constraintTop_toBottomOf="@id/tv_profile_id"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="19dp"
        app:shapeAppearance="@style/ShapeAppearance.Material3.Corner.Full"/>

    <TextView
        android:id="@+id/tv_profile_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="쿠잇"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="13dp"
        app:layout_constraintTop_toBottomOf="@id/iv_profile_img"
        android:layout_marginTop="12dp"
        android:textColor="@color/black"
        />
    <TextView
        android:id="@+id/tv_post_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/iv_profile_img"
        android:layout_marginStart="55dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="92dp"
        />
    <TextView
        android:id="@+id/tv_follower_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/iv_profile_img"
        android:layout_marginStart="126dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="92dp"
        />
    <TextView
        android:id="@+id/tv_following_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/iv_profile_img"
        android:layout_marginStart="199dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="92dp"
        />
    <TextView
        android:id="@+id/tv_post"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="게시물"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="144dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="109dp"
        />
    <TextView
        android:id="@+id/tv_follower"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="팔로워"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="216dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="110dp"
        />
    <TextView
        android:id="@+id/tv_following"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="팔로잉"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="289dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="109dp"
        />
    <ImageView
        android:id="@+id/iv_profile_btn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_profie_edit"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="13dp"
        app:layout_constraintTop_toBottomOf="@id/tv_profile_name"
        android:layout_marginTop="9dp"
        />
    <TextView
        android:id="@+id/tv_profile_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프로필 편집"
        android:textSize="11sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_profile_btn1"
        android:layout_marginStart="47dp"
        app:layout_constraintTop_toTopOf="@id/iv_profile_btn1"
        android:layout_marginTop="8dp"
        />
    <ImageView
        android:id="@+id/iv_profile_btn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_profie_edit"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="165dp"
        app:layout_constraintTop_toTopOf="@id/iv_profile_btn1"
        />
    <TextView
        android:id="@+id/tv_profile_share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프로필 공유"
        android:textSize="11sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/iv_profile_btn2"
        android:layout_marginStart="47dp"
        app:layout_constraintTop_toTopOf="@id/iv_profile_btn1"
        android:layout_marginTop="8dp"
        />
    <ImageView
        android:id="@+id/iv_btn_people"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_profile_people"
        app:layout_constraintStart_toEndOf="@id/iv_profile_btn2"
        android:layout_marginStart="4dp"
        app:layout_constraintTop_toTopOf="@id/iv_profile_btn1"
        />
    <ImageView
        android:id="@+id/iv_ic_people"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_people"
        app:layout_constraintStart_toStartOf="@id/iv_btn_people"
        android:layout_marginStart="3dp"
        app:layout_constraintTop_toTopOf="@id/iv_btn_people"
        android:layout_marginTop="3dp"
        />



</androidx.constraintlayout.widget.ConstraintLayout>