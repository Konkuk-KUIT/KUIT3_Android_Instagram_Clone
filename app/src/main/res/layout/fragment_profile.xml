<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchFragment">

    <ImageView
        android:id="@+id/ic_profile_margin"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/ic_graycircle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </ImageView>
    <ImageView
        android:id="@+id/ic_profile_margin_colored"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/ic_coloredcircle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </ImageView>

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/ic_profile1"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:layout_marginHorizontal="5dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/img_sample"
        app:layout_constraintStart_toStartOf="@id/ic_profile_margin"
        app:layout_constraintTop_toTopOf="@id/ic_profile_margin"
        app:shapeAppearance="@style/ShapeAppearance.Material3.Corner.Full">

    </com.google.android.material.imageview.ShapeableImageView>

</androidx.constraintlayout.widget.ConstraintLayout>