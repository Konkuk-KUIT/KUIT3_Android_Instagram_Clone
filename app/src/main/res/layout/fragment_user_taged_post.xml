<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".UserTagedPostFragment">
    <GridLayout
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:columnCount="3"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/iv_post_1"></ImageView>
        <ImageView
            android:id="@+id/iv_post_2"></ImageView>
        <ImageView
            android:id="@+id/iv_post_3"></ImageView>
        <ImageView
            android:id="@+id/iv_post_4"></ImageView>
        <ImageView
            android:id="@+id/iv_post_5"></ImageView>
        <ImageView
            android:id="@+id/iv_post_6"></ImageView>

    </GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>